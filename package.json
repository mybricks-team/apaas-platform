{
  "name": "mybricks-apaas-platform",
  "version": "0.1.32",
  "description": "",
  "scripts": {
    "docker:load": "docker load < ./mybricks-apaas.tar",
    "docker:save": "docker save -o ./mybricks-apaas.tar mybricks-apaas:v0.1.12",
    "docker:build:linux": "docker buildx build --platform linux/amd64 -t mybricks-apaas:v0.1.12 .",
    "docker:build:mac": "docker buildx build --platform linux/arm64/v8 -t mybricks-apaas:v0.0.1 .",
    "docker:run": "docker run -p 14100:4100 -v ~/Work/registry/mybricks-team/apaas-platform/PlatformConfig.json:/home/apaas/external/PlatformConfig.json -v ~/Work/registry/mybricks-team/apaas-platform/logs:/home/apaas/logs -v ~/Work/registry/mybricks-team/apaas-platform/_localstorage:/home/apaas/_localstorage -v ~/Work/registry/mybricks-team/apaas-platform/_apps:/home/apaas/_apps -d --name mybricks-apaas mybricks-apaas:v0.1.12",
    "docker:runtest": "docker run -p 14100:4100 -v ~/Work/registry/test/PlatformConfig.json:/home/apaas/external/PlatformConfig.json -v ~/Work/registry/test/logs:/home/apaas/logs -v ~/Work/registry/test/_localstorage:/home/apaas/_localstorage -v ~/Work/registry/test/_apps:/home/apaas/_apps -d --name mybricks-apaas mybricks-apaas:v0.0.1",
    "docker:stop": "docker stop mybricks-apaas",
    "docker:tag": "docker tag mybricks-apaas:v0.1.12 andyzou/mybricks-apaas:v0.1.12",
    "docker:push": "docker push andyzou/mybricks-apaas:v0.1.12",
    "kube:startService": "kubectl apply -f ./mybricks-apaas-pod.yaml && kubectl apply -f ./mybricks-apaas-service.yaml",
    "kube:getAllPods": "kubectl get pods",
    "kube:getService": "kubectl get services",
    "kube:getDetail": "kubectl get pods -o wide",
    "kube:deleteAll": "kubectl delete deployments,services,pods --all",
    "build:page": "cd page && npm install && npm run build",
    "zip:update": "npm run build:page && node ./zip_update.js",
    "zip:deploy": "npm run build:page && node ./zip_deploy.js",
    "zip:offline": "npm run build:page && cd server && npm i && cd ../ && node ./zip_offline.js",
    "deploy:fangzhou": "bash deploy_fangzhou.sh",
    "publishToNpm": "npm run zip:update && npm publish --registry=https://registry.npmjs.org && node sync.js --origin=https://my.mybricks.world"
  },
  "files": [
    "mybricks-apaas.zip",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:mybricks-team/official-website.git"
  },
  "author": "MyBricks Team",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "axios": "^1.4.0",
    "jszip": "^3.10.1"
  }
}
